{
  "version": 2,
  "name": "gaby.dev",
  "scope": "squared",
  "public": false,
  "github": {
    "enabled": true,
    "autoAlias": true,
    "silent": true,
    "autoJobCancelation": true
  },
  "routes": [
    {
      "src": "/blog",
      "status": 301,
      "headers": { "Location": "https://gaby.dev/posts" }
    },
    {
      "src": "/github",
      "status": 302,
      "headers": { "Location": "https://github.com/GitSquared" }
    },
    {
      "src": "/donate",
      "status": 302,
      "headers": { "Location": "https://buymeacoffee.com/gaby" }
    },
    {
      "src": "/edex",
      "status": 302,
      "headers": { "Location": "https://github.com/GitSquared/edex-ui" }
    },
    {
      "src": "/edex-vuln",
      "status": 302,
      "headers": { "Location": "https://squared.codebrew.fr/posts/edex-injection-vuln/" }
    }
  ]
}
