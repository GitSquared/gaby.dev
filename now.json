{
  "version": 2,
  "name": "gaby.dev",
  "alias": "gaby.dev",
  "scope": "squared",
  "regions": ["cdg", "sfo", "gru", "cle", "bom", "hnd", "syd"],
  "public": false,
  "github": {
    "enabled": true,
    "autoAlias": true,
    "silent": true,
    "autoJobCancelation": true
  },
  "builds": [
    { "src": "redirect/index.js", "use": "@now/node" }
  ],
  "routes": [
    {
      "src": "/",
      "status": 302,
      "headers": { "Location": "https://squared.codebrew.fr" }
    },
    { "src": "/(?<path>.+)", "dest": "/redirect/index.js?path=$path" }
  ]
}
